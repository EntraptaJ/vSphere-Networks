{
  "name": "vSphere Networks",
  "dockerFile": "Dockerfile.dev",
  "shutdownAction": "none",
  "settings": {
    // This dev container does include /bin/bash if you prefer to use it instead of ash.
    "terminal.integrated.shell.linux": "/bin/sh"
  },
  "remoteEnv": {
    "SHELL": "/bin/sh"
  },
  "extensions": ["esbenp.prettier-vscode"],
  "remoteUser": "node"
}
